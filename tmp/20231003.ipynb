    "def decorator(f):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        kwargs['new_var'] = 'Injected variable'\n",
    "        return f(*args, **kwargs)\n",
    "    return wrapper\n",
    "\n",
    "@decorator\n",
    "def test_function(a, b, new_var=None):\n",
    "    print(a, b, new_var)\n",
    "\n",
    "test_function(1, 2)"
